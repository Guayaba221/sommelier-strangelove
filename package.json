{
  "name": "somm-boilerplate",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build": "next build",
    "cy:open": "cypress open",
    "cy:record": "cypress run --record",
    "cy:run": "cypress run",
    "deadcode": "ts-prune",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "chakra:gen": "chakra-cli tokens ./src/theme/index.ts",
    "postinstall": "yarn chakra:gen",
    "generate": "graphql-codegen",
    "generate:watch": "yarn generate --watch",
    "generate:schema": "get-graphql-schema https://api.thegraph.com/subgraphs/name/elkdao/cellars > src/generated/schema.json --json"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@cosmjs/cosmwasm-stargate": "^0.27.1",
    "@emotion/react": "11.8.1",
    "@emotion/styled": "11.8.1",
    "@ethersproject/bignumber": "^5.5.0",
    "@fontsource/oswald": "^4.5.4",
    "@keplr-wallet/types": "^0.9.12",
    "@next/bundle-analyzer": "^12.1.0",
    "@nivo/bar": "^0.79.1",
    "@nivo/core": "^0.79.0",
    "@nivo/line": "^0.79.1",
    "@sentry/nextjs": "6.18.2",
    "ethers": "^5.5.4",
    "framer-motion": "^6.2.6",
    "graphql": "^16.3.0",
    "next": "^12.1.0",
    "next-plausible": "^3",
    "react": "^17",
    "react-dom": "^17",
    "react-hook-form": "^7.27.0",
    "react-icons": "^4",
    "react-query": "^3.34.15",
    "urql": "^2.2.0",
    "wagmi": "^0.2.12",
    "zustand": "^3.7.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "1.9.0",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-urql": "^3.5.3",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "cypress": "^9.5.0",
    "eslint": "8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "get-graphql-schema": "^2.1.2",
    "prettier": "^2",
    "ts-prune": "^0",
    "typescript": "4.6.2",
    "url": "^0"
  },
  "private": true,
  "volta": {
    "node": "16.14.0",
    "yarn": "1.22.17"
  }
}
